program Project2;

{$APPTYPE CONSOLE}

uses
  SysUtils,
  UStack in 'UStack.pas';
var f:textfile;
s:tStack;
c:char;
begin
  if not fileexists('input.txt') then
    writeln('File input.txt not found')
  else begin
    assignfile(f, 'input.txt');
    reset(f);

    Init(s);
    while not eof(f) do begin
      read(f,c);
      push(s,c);
    end;
    close(f);
    assignfile(f, 'output.txt');
    rewrite(f);
    while not isempty(s) do
      write(f, pop(s));
    close(f);
    writeln('Done');
  end;
  readln
end.
